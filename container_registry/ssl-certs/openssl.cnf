[ req ]
default_bits        = 2048
distinguished_name  = req_distinguished_name
req_extensions      = req_ext
prompt              = no

[ req_distinguished_name ]
C                   = US
ST                  = State
L                   = City
O                   = Organization
OU                  = Department
CN                  = 192.168.56.3   # Common Name (use IP address here)

[ req_ext ]
subjectAltName      = @alt_names

[ alt_names ]
IP.1                = 192.168.56.3  # IP Address

